          <colspec colname="c3" colwidth="1*"/>
          <thead>
            <row>
              <entry>Release</entry>
              <entry>Internal services image tag</entry>
              <entry>ZooKeeper image tag</entry>
            </row>
          </thead>
          <tbody>
            <row>
              <entry><ph keyref="nm-cc"/> 1.2.3</entry>
              <entry>zenoss/serviced-isvcs:v??</entry>
